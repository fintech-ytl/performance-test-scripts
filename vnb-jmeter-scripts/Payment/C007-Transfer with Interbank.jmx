          <stringProp name="BeanShellSampler.query">//创建建文对象
FileWriter fw =new FileWriter(&quot;C:\\Project\\data\\C7.txt&quot;,false);
//创建一个字符缓存斯出流
BufferedWriter bufw=new BufferedWriter (fw);
//获取sal 查询结果的行数
String num=vars.get(&quot;customerId_#&quot;);
//字符串转换成int
int count = Integer.parseInt(num);
//制循环次数，写入count行文件
for(int i=1;i&lt;=count;i++){
//获取sqL查询结果第i行的数据
    str_CusId=vars.get(&quot;customerId_&quot;+i);
    str_FuOpId=vars.get(&quot;fundOptionId_&quot;+i);
    str_AccId=vars.get(&quot;accountId_&quot;+i);
//将教据写入磁盘文件中
    bufw.write(str_CusId+&apos;,&apos;);
    bufw.write(str_FuOpId+&apos;,&apos;);
    bufw.write(str_AccId);
//换行
    bufw.write(&quot;\r\n&quot;);
}
//文件写入结束后，关闭流
bufw.close();
fw.close();
</stringProp>
        </BeanShellSampler>
